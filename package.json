{
  "name": "nestjs-swagger-no-ui",
  "version": "7.1.19",
  "description": "This is a forked version of @nestjs/swagger which has removed the ui dependency",
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/eglenceco/nestjs-swagger-no-ui",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier \"lib/**/*.ts\" --write",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:e2e": "jest --config e2e/jest-e2e.json",
    "prerelease": "npm run build",
    "release": "release-it",
    "---manual-tests---": "",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --watch --debug"
  },
  "dependencies": {
    "@nestjs/mapped-types": "2.1.0",
    "js-yaml": "4.1.1",
    "lodash": "4.17.21",
    "path-to-regexp": "8.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-angular": "20.3.1",
    "@fastify/static": "9.0.0",
    "@nestjs/common": "11.1.12",
    "@nestjs/core": "11.1.12",
    "@nestjs/platform-express": "11.1.12",
    "@nestjs/platform-fastify": "11.1.12",
    "@types/jest": "30.0.0",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.17.23",
    "@types/node": "25.0.8",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "express": "5.2.1",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "openapi-types": "12.1.3",
    "prettier": "3.8.0",
    "reflect-metadata": "0.2.2",
    "release-it": "19.2.3",
    "supertest": "7.2.2",
    "swagger-parser": "10.0.3",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@fastify/static": "^6.0.0",
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "class-transformer": "*",
    "class-validator": "*",
    "reflect-metadata": "^0.1.12"
  },
  "peerDependenciesMeta": {
    "@fastify/static": {
      "optional": true
    },
    "class-transformer": {
      "optional": true
    },
    "class-validator": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
